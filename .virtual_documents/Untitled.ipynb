import os
import pandas as pd

# Create folder to save dataset
save_dir = os.path.expanduser("~/Project/AttnLstm/data/raw/iflex")
os.makedirs(save_dir, exist_ok=True)

# Dataset URL (replace if needed)
url = "https://zenodo.org/record/5744261/files/iFlex_consumption_hourly.csv"
file_path = os.path.join(save_dir, "iFlex_consumption_hourly.csv")

# Download dataset if it doesn't exist
if not os.path.exists(file_path):
    df = pd.read_csv(url)
    df.to_csv(file_path, index=False)
    print(f"Dataset saved to {file_path}")
else:
    print(f"Dataset already exists at {file_path}")







